<h1>All songs</h1>
<table class="table table-hover ma-player-table">
    <thead>
        <tr>
            <th></th>
            <th>Track</th>
            <th></th>
        </tr>
    </thead>

  <tbody>
    <% @musics.each do |music| %>
      <tr class="ma-player-table-tr">
        <td class="ma-player-table-cover">
            <%= image_tag(music.cover.url, class: "img-responsive ma-player-list-cover") %>
            <button class="btn btn-primary ma-player-list-play" onclick="Player.play(<%= music.id %>);"><%= fa_icon "play" %></button>
        </td>
        <td>
            <strong><%= music.artist %></strong><br/>
            <%= music.title %>
        </td>
        <td>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>
